    "PrimeNG AutoComplete Reactive Form Component": {
      "prefix": "p-autocomplete-reactive-form-component",
      "body": [
        "import { Component, OnInit } from \"@angular/core\";",
        "import { FormControl, FormGroup, ReactiveFormsModule } from '@angular/forms';",
        "import { AutoComplete } from 'primeng/autocomplete';",
        "",
        "interface AutoCompleteCompleteEvent {",
        "    originalEvent: Event;",
        "    query: string;",
        "}",
        "",
        "@Component({",
        "    selector: \"app-${1:component-name}\",",
        "    template: `",
        "        <form [formGroup]=\"formGroup\">",
        "            <p-autocomplete formControlName=\"selectedCountry\"",
        "                [suggestions]=\"countries\"",
        "                (completeMethod)=\"search($event)\" />",
        "        </form>",
        "    `,",
        "    styleUrl: \"./${1:component-name}.component.css\",",
        "    standalone: true,",
        "    imports: [ReactiveFormsModule, AutoComplete]",
        "})",
        "",
        "export class ${2:ComponentName} implements OnInit {",
        "",
        "    formGroup!: FormGroup;",
        "    countries: string[] = [];",
        "    allCountries: string[] = [",
        "        \"United States\", \"Canada\", \"United Kingdom\", \"Germany\", \"France\",",
        "        \"Italy\", \"Spain\", \"Netherlands\", \"Australia\", \"Japan\",",
        "        \"China\", \"India\", \"Brazil\", \"Mexico\", \"South Korea\",",
        "        \"Russia\", \"Turkey\", \"Sweden\", \"Switzerland\", \"South Africa\"",
        "    ];",
        "",
        "    search(event: AutoCompleteCompleteEvent) {",
        "        this.countries = this.allCountries.filter(item =>",
        "            item.toLowerCase().includes(event.query.toLowerCase())",
        "        );",
        "    }",
        "",
        "    ngOnInit() {",
        "        this.formGroup = new FormGroup({",
        "            selectedCountry: new FormControl(null),",
        "        });",
        "    }",
        "}"
