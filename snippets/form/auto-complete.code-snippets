    "PrimeNG AutoComplete Dropdown Component": {
      "prefix": "p-autocomplete-dropdown-component",
      "body": [
        "import { Component } from \"@angular/core\";",
        "import { AutoComplete } from 'primeng/autocomplete';",
        "import { FormsModule } from '@angular/forms';",
        "",
        "interface AutoCompleteCompleteEvent {",
        "    originalEvent: Event;",
        "    query: string;",
        "}",
        "",
        "@Component({",
        "    selector: \"app-${1:component-name}\",",
        "    template: `",
        "        <p-autocomplete [(ngModel)]=\"value\"",
        "            [dropdown]=\"true\"",
        "            [suggestions]=\"items\"",
        "            (completeMethod)=\"search($event)\" />",
        "    `,",
        "    styleUrl: \"./${1:component-name}.component.css\",",
        "    standalone: true,",
        "    imports: [FormsModule, AutoComplete]",
        "})",
        "",
        "export class ${2:ComponentName} {",
        "    items: any[] = [];",
        "    value: any;",
        "",
        "    search(event: AutoCompleteCompleteEvent) {",
        "        let _items = [...Array(10).keys()];",
        "        this.items = event.query ? [...Array(10).keys()].map((item) => event.query + '-' + item) : _items;",
        "    }",
        "}"
      ],
      "description": "Snippet for PrimeNG AutoComplete Dropdown Component"
