    "PrimeNG AutoComplete Component": {
      "prefix": "p-autocomplete-component",
      "body": [
        "import { Component } from \"@angular/core\";",
        "import { ButtonModule } from 'primeng/button';",
        "import { RouterModule } from \"@angular/router\";",
        "import { AutoComplete } from 'primeng/autocomplete';",
        "import { FormsModule } from '@angular/forms';",
        "",
        "interface AutoCompleteCompleteEvent {",
        "    originalEvent: Event;",
        "    query: string;",
        "}",
        "",
        "@Component({",
        "    selector: \"app-${1:component-name}\",",
        "    template: `",
        "        <p-autocomplete [(ngModel)]=\"value\" [suggestions]=\"items\"",
        "            (completeMethod)=\"search($event)\" appendTo=\"body\"/>",
        "    `,",
        "    styleUrl: \"./${1:component-name}.component.css\",",
        "    standalone: true,",
        "    imports: [ButtonModule, RouterModule, AutoComplete, FormsModule]",
        "})",
        "",
        "export class ${2:ComponentName} {",
        "",
        "    items: any[] = [];",
        "    allItems: any[] = [",
        "        \"Foo\", \"Bar\", \"Baz\", \"Qux\",",
        "        \"Quux\", \"Corge\", \"Grault\", \"Garply\",",
        "        \"Waldo\", \"Fred\", \"Plugh\", \"Xyzzy\"",
        "    ];",
        "",
        "    value: any;",
        "",
        "    search(event: AutoCompleteCompleteEvent) {",
        "        this.items = this.allItems.filter(item =>",
        "            item.toLowerCase().includes(event.query.toLowerCase())",
        "        );",
        "    }",
        "",
        "}"
