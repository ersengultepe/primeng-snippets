            "                            <p-slider [(ngModel)]=\"${10:activityValues}\" [range]=\"true\" (onSlideEnd)=\"filter($event.values)\" styleClass=\"m-4\" />",
            "                            <div class=\"flex items-center px-2\">",
            "                                <span>{{ activityValues[0] }}</span>",
            "                                <span>{{ activityValues[1] }}</span>",
            "                            </div>",
            "                        </ng-template>",
            "                    </p-columnFilter>",
