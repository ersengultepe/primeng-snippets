            "                        <ng-template pTemplate=\"filter\" let-value let-filter=\"filterCallback\">",
            "                            <p-dropdown [(ngModel)]=\"value\" [options]=\"${7:statuses}\" (onChange)=\"filter($event.value)\" placeholder=\"${8:Any}\">",
            "                                <ng-template let-option pTemplate=\"item\">",
            "                                    <p-tag [value]=\"option.value\" [severity]=\"${9:getSeverity(option.label)}\" />",
