            "                        <ng-template pTemplate=\"header\">",
            "                            <div class=\"px-4 pt-4 pb-0\">",
            "                                <span class=\"font-bold\">Agent Picker</span>",
            "                            </div>",
            "                        </ng-template>",
            "                        <ng-template pTemplate=\"filter\" let-value let-filter=\"filterCallback\">",
            "                            <p-multiSelect [(ngModel)]=\"value\" [options]=\"${5:representatives}\" placeholder=\"${6:Any}\" (onChange)=\"filter($event.value)\" optionLabel=\"name\">",
            "                                <ng-template let-option pTemplate=\"item\">",
            "                                    <div class=\"inline-block align-middle\">",
            "                                        <img [alt]=\"option.label\" src=\"https://primefaces.org/cdn/primeng/images/demo/avatar/{{ option.image }}\" width=\"24\" class=\"align-middle\" />",
            "                                        <span class=\"ml-1 mt-1\">{{ option.name }}</span>",
            "                                    </div>",
            "                                </ng-template>",
            "                            </p-multiSelect>",
            "                        </ng-template>",
            "                    </p-columnFilter>",
            "                </div>",
