            "        <tr>",
            "            <th>Code</th>",
            "            <th>Name</th>",
            "            <th>Category</th>",
            "            <th>Quantity</th>",
            "        </tr>",
            "    </ng-template>",
            "    <ng-template pTemplate=\"body\" let-product>",
            "        <tr>",
